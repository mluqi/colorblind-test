{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "eol-last": 0,
        "indent": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "operator-linebreak": 0,
        "space-before-blocks": 0,
        "no-loop-func": 0,
        "import/extensions": 0,
        "arrow-parens": 0,
        "no-undef":0,
        "no-return-assign":0,
        "no-plusplus":0,
        "no-use-before-define":0,
        "no-unused-vars":0,
        "import/no-unresolved":0,
        "import/no-extraneous-dependencies":0,
        "eqeqeq":0,
        "no-param-reassign":0
    }
}
