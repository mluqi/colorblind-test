{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "eol-last": 0,
        "indent": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "operator-linebreak": 0,
        "space-before-blocks": 0
    }
}
